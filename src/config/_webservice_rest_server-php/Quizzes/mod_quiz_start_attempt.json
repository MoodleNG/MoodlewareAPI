{
  "method": "POST",
  "description": "Start a new quiz attempt",
  "query_params": [
    {
      "name": "quizid",
      "type": "int",
      "required": true,
      "description": "Quiz ID"
    },
    {
      "name": "preflightdata",
      "type": "list",
      "required": false,
      "default": [],
      "description": "Pre-flight data as list of {name, value} objects (JSON or CSV)."
    },
    {
      "name": "forcenew",
      "type": "bool",
      "required": false,
      "default": false,
      "description": "Force new attempt."
    }
  ],
  "responses": {
    "200": {
      "description": "Attempt started",
      "content": {
        "application/json": {
          "schema": {
            "type": "object"
          },
          "example": {
            "attempt": {
              "id": 101,
              "quiz": 15,
              "state": "inprogress",
              "timestart": 1694350000,
              "layout": "1,2,3,0"
            },
            "warnings": []
          }
        }
      }
    }
  }
}
